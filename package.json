{
  "name": "@solid-soda/evolutions",
  "version": "0.0.10",
  "description": "Simple library providing scala-play like evolutions",
  "scripts": {
    "s": "yarn soda"
  },
  "bin": {
    "evolutions": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solid-soda/trona-evolutions.git"
  },
  "keywords": [
    "sql",
    "database",
    "migrations"
  ],
  "author": "Stanislav Golikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solid-soda/trona-evolutions/issues"
  },
  "homepage": "https://github.com/solid-soda/trona-evolutions#readme",
  "dependencies": {
    "command-line-args": "^5.0.2",
    "fs": "0.0.1-security",
    "md5": "^2.2.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@solid-soda/scripts": "^1.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn soda lint"
    ],
    "*.{js,jsx,ts,tsx,css,json}": [
      "yarn soda pretty",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
